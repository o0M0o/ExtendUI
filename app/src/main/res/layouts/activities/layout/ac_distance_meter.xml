<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="wxm.extendui.DistanceMeter.ACDistanceMeter">

    <wxm.uilib.DistanceMeter.DistanceMeter
        android:id="@+id/dm_obj"
        android:layout_width="0dp"
        android:layout_height="84dp"
        android:paddingEnd="4dp"
        android:paddingStart="4dp"
        app:dmMainScaleCount="5"
        app:dmMainScaleHeight="32dp"
        app:dmSubScaleCount="10"
        app:dmSubScaleHeight="20dp"
        app:dmTagWidth="6dp"
        app:dmTagHeight="10dp"
        app:dmTextSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
