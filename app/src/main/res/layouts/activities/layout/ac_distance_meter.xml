<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="wxm.extendui.DistanceMeter.ACDistanceMeter">

    <wxm.uilib.DistanceMeter.DistanceMeter
        android:id="@+id/dm_obj"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:paddingEnd="4dp"
        android:paddingStart="4dp"
        app:dmMainScaleCount="6"
        app:dmMainScaleHeight="38dp"
        app:dmSubScaleCount="8"
        app:dmSubScaleHeight="28dp"
        app:dmTagWidth="12dp"
        app:dmTagHeight="10dp"
        app:dmTextSize="12sp"
        app:dmMaxValue="120"
        app:dmMinValue="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
